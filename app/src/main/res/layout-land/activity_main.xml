<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/container"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.administrator.calculator.MainActivity"
    android:orientation="vertical"
    android:weightSum="1">

    <TextView
        android:id="@+id/textView2"
        android:layout_width="match_parent"
        android:layout_height="0dip"
        android:layout_weight="0.82"
        android:textSize="28sp"
        android:gravity="end|bottom"
        android:hint="@string/TestView_name"
        android:background="@drawable/back_dp"/>
    <TableLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

    <TableRow
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <Button
            android:id="@+id/bt_C"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:text="@string/bt_C" />

        <Button
            android:id="@+id/bt_div"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:text="@string/bt_div" />

        <Button
            android:id="@+id/bt_multiply"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:text="@string/bt_multiply" />

        <Button
            android:id="@+id/bt_DEL"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:text="@string/bt_DEL" />

        <Button
            android:id="@+id/bt_square"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:text="@string/bt_square" />

        <Button
            android:id="@+id/bt_cube"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:text="@string/bt_cube" />

        <Button
            android:id="@+id/bt_Nth"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:text="@string/bt_Nth" />


    </TableRow>

    <TableRow
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <Button
            android:id="@+id/bt_7"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:text="@string/bt_7" />

        <Button
            android:id="@+id/bt_8"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:text="@string/bt_8" />

        <Button
            android:id="@+id/bt_9"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:text="@string/bt_9" />

        <Button
            android:id="@+id/bt_sub"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:text="@string/bt_sub" />
        <Button
            android:id="@+id/bt_cos"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:text="@string/bt_cos" />

        <Button
            android:id="@+id/bt_sin"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:text="@string/bt_sin" />

        <Button
            android:id="@+id/bt_tan"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:text="@string/bt_tan" />


    </TableRow>

    <TableRow
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <Button
            android:id="@+id/bt_4"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:text="@string/bt_4" />

        <Button
            android:id="@+id/bt_5"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:text="@string/bt_5" />

        <Button
            android:id="@+id/bt_6"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:text="@string/bt_6" />

        <Button
            android:id="@+id/bt_add"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:text="@string/bt_add" />
        <Button
            android:id="@+id/bt_factory"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:text="@string/bt_factory" />

        <Button
            android:id="@+id/bt_sqrt"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:text="@string/bt_sqrt" />

        <Button
            android:id="@+id/bt_dao"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:text="@string/bt_dao" />

    </TableRow>

    <TableRow
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <Button
            android:id="@+id/bt_1"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:text="@string/bt_1" />

        <Button
            android:id="@+id/bt_2"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:text="@string/bt_2" />

        <Button
            android:id="@+id/bt_3"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:text="@string/bt_3" />

        <Button
            android:id="@+id/bt_equals"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:text="@string/bt_equals" />
        <Button
            android:id="@+id/bt_E"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:text="@string/bt_E" />

        <Button
            android:id="@+id/bt_ln"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:text="@string/bt_ln" />

        <Button
            android:id="@+id/bt_lg"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:text="@string/bt_lg" />


    </TableRow>


        <TableRow
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <Button
                android:id="@+id/bt_per"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:text="@string/bt_per" />

            <Button
                android:id="@+id/bt_0"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:text="@string/bt_0" />

            <Button
                android:id="@+id/bt_point"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:text="@string/bt_point" />

        </TableRow>
</TableLayout>
</LinearLayout>